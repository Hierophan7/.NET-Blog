@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="">
	<div class="container-fluid">
		<div class="row">
			<div class="col">
				<div class="row justify-content-center">
					<div class="col-4">
						<form class="navbar-form" asp-action="Index" asp-controller="Home">
							<div class="input-group no-border">
								<input type="text" name="searchString" class="form-control" placeholder="Search...">
								<button type="submit" class="btn btn-default btn-round btn-just-icon">
									<i class="material-icons">search</i>
									<div class="ripple-container"></div>
								</button>
							</div>
						</form>
					</div>
				</div>

				<div class="row justify-content-center">
					<div class="col-4">
						<img src="~/assets/img/QuarterlyThirdBlacklab-small.gif" />
					</div>
				</div>

				<div class="row justify-content-center">
					<div class="col-4">
						<h3 class="text-muted text-center">No Results Found</h3>
					</div>
				</div>

				<div class="row justify-content-center">
					<div class="col-4">
						<h4 class="text-muted text-center">Please update your search and try again.</h4>
					</div>
				</div>
			</div>
		</div>
		
	</div>
</div>